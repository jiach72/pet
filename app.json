{
    "expo": {
        "name": "PetPulse",
        "slug": "petpulse",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#3B82F6"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.petpulse.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#3B82F6"
            },
            "package": "com.petpulse.app"
        },
        "web": {
            "bundler": "metro",
            "output": "static"
        },
        "plugins": [
            "expo-router",
            "expo-font"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "scheme": "petpulse"
    }
}